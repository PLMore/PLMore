{
  "name": "EzyFS",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/web-project/test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.6.7",
    "@grpc/proto-loader": "0.6.13",
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/apollo": "^10.0.16",
    "@nestjs/bull": "0.5.5",
    "@nestjs/common": "8.2.5",
    "@nestjs/config": "1.2.1",
    "@nestjs/core": "8.2.5",
    "@nestjs/graphql": "^10.0.16",
    "@nestjs/jwt": "8.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "8.2.6",
    "@nestjs/passport": "8.1.0",
    "@nestjs/platform-express": "8.2.5",
    "@nestjs/schedule": "1.1.0",
    "@nestjs/typeorm": "8.0.3",
    "@types/crypto-js": "4.1.1",
    "@types/openpgp": "4.4.18",
    "@types/tar": "6.1.1",
    "apollo-server-cache-redis": "3.3.1",
    "apollo-server-express": "3.10.1",
    "bcrypt": "5.0.1",
    "bull": "4.8.4",
    "cache-manager": "4.0.1",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.2",
    "dotenv": "16.0.1",
    "eslint-config-airbnb": "18.2.1",
    "fs": "0.0.1-security",
    "fstream": "1.0.12",
    "graphql": "^16.5.0",
    "graphql-passport": "0.6.4",
    "gulp": "4.0.2",
    "module-alias": "2.2.2",
    "nestjs-consul": "1.4.0",
    "node-fetch": "3.2.0",
    "node-gyp": "9.0.0",
    "node-rsa": "1.1.1",
    "nodemailer": "6.7.5",
    "openpgp": "5.1.0",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "path": "0.12.7",
    "pg": "8.7.1",
    "pg-connection-string": "2.5.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "stream-promise": "3.2.0",
    "tar": "6.1.11",
    "typeorm": "0.3.20",
    "uuid": "8.3.2",
    "zlib": "1.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.0",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.2.5",
    "@types/bull": "3.15.8",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/module-alias": "2.0.1",
    "@types/node": "16.11.41",
    "@types/node-rsa": "1.1.1",
    "@types/nodemailer": "6.4.4",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-typescript": "14.0.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.1",
    "husky": "8.0.1",
    "jest": "27.4.7",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "supertest": "6.2.3",
    "ts-jest": "27.1.3",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@crypto/crypto(|/.*)$": "<rootDir>/libs/crypto/src/$1",
      "^@common/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@ezyfs/internal(|/.*)$": "<rootDir>/libs/internal/src/$1",
      "^@ezyfs/proto-schema(|/.*)$": "<rootDir>/libs/proto-schema/src/$1",
      "^@repositories/repositories(|/.*)$": "<rootDir>/libs/repositories/src/$1"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
